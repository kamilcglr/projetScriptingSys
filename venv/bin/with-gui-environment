# -*- coding: utf-8 -*-
import re
import sys

from proc.notify import with_gui_environment

if __name__ == '__main__.py':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(with_gui_environment())
